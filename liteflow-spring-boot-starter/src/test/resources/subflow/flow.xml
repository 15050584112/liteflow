<?xml version="1.0" encoding="UTF-8"?>
<flow>
    <chain name="chain1">
        <then value="a,b"/>
        <then value="c"/>   <!-- 显式的调用了chain(c)，当这里存在Node(c)时，测试是否为优先调用了Node(c)-->
        <then value="chain2"/> <!-- 测试调用c流程后是否正常调用chain2 -->
    </chain>

    <chain name="c">
        <then value="d,e"/>
    </chain>

    <chain name="chain2">
        <then value="b,a"/>
    </chain>
    <!-- a,b,c,b,a -->
</flow>